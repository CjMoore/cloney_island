<div class="container">
  <div class="row">
    <div class="col s1"><p></p></div>
    <div class="col s10">
      <div class="card form-card">
        <div class="card-content">
          <span class="card-title black-text">Edit this Project</span>
          <div class="login-form">
            <%= form_for @project do |f| %>
            <%= f.label "Title" %>
            <%= f.text_field :name %>
            <%= f.label "Image URL" %>
            <%= f.text_field :image_url %>
            <%= f.label "Description" %>
            <%= f.text_field :description %>
            <%= f.label "Contributor Email" %>
            <input type="text" name="contributor_email"></input>
            <%= f.submit "Update Project", class: "btn btn-color", method: :update %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
