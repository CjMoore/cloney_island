<div class="container">
  <h1>Start a Project</h1>

<%= form_for :projects do |f| %>
<%= f.label "Title" %>
<%= f.text_field :content %>
<%= f.label "Image URL" %>
<%= f.text_field :content, placeholder: "www.image.com/your_image" %>
<%= f.label "Description" %>
<%= f.text_field :content %>
<%= f.label "Total Time" %>
<select class="browser-default" name= "time[month]">
  <option value="7">1 Week</option>
  <option value="31">1 Month</option>
  <option value="90">3 Months</option>
</select><br>
<%= f.label "Amount to Raise" %>
<%= f.text_field :content %>
<%= f.label "Contributor Email" %>
<%= f.text_field :content %>
<%= f.submit "Create Project", class: "btn btn-color" %>
<% end %>

</div>
